{
    "name": "peerme/mx-sdk-laravel",
    "description": "MultiversX SDK for Laravel (written in PHP).",
    "keywords": [
        "multiversx",
        "laravel"
    ],
    "homepage": "https://github.com/peermehq/mx-sdk-laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Micha Vie",
            "email": "micha@freeiam.com",
            "homepage": "https://michavie.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "bitwasp/bech32": "^0.0.1",
        "brick/math": "^0.9.3",
        "guzzlehttp/guzzle": "^7.4.2",
        "illuminate/contracts": "^9.9.0",
        "illuminate/http": "^9.9.0",
        "illuminate/support": "^9.9.0",
        "kornrunner/keccak": "^1.1",
        "simplito/elliptic-php": "^1.0"
    },
    "require-dev": {
        "orchestra/testbench": "^7.4.0",
        "pestphp/pest": "^1.21.2",
        "pestphp/pest-plugin-laravel": "^1.2.0",
        "phpunit/phpunit": "^9.5.20",
        "spatie/pest-plugin-test-time": "^1.0",
        "spatie/phpunit-snapshot-assertions": "^4.2"
    },
    "autoload": {
        "psr-4": {
            "Peerme\\Multiversx\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Peerme\\Multiversx\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Peerme\\Multiversx\\ServiceProvider"
            ]
        }
    },
    "funding": [
        {
            "type": "github",
            "url": "https://github.com/sponsors/peermehq"
        }
    ]
}
